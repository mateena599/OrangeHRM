package com.BasicJava;

public class IfElse {

	public static void main(String[] args)
	{
		int a=10,b=20,c=90;
		 
		if(a>b && a>c)
		{
			System.out.println("a is greater than b");
		}
		
		else if(b>c && b >a)
		{
			System.out.println("b is greater than a");
		}
		else
		{
			System.out.println("c is greater ");

		}

	}

}
